@model IEnumerable<SIRESEP.DATOS.tbCarrera>

@{
    ViewBag.Title = "Index";
}

<h2>Carreras</h2>

<p>
    @Html.ActionLink("Nueva Carrera", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idCarrera)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.carrera)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idCarrera)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.carrera)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.idCarrera }) |
            @Html.ActionLink("Detalles", "Details", new { id = item.idCarrera }) |
            @Html.ActionLink("Eliminar", "Delete", new { id = item.idCarrera }, new { onclick = "return confirm('Desea eliminar esta carrera?');" })
        </td>
    </tr>
}

</table>
