@model IEnumerable<SIRESEP.DATOS.tbInfoIdioma>

@{
    ViewBag.Title = "Index";
}

<h2>Informacion de Idioma</h2>


<p>
    @Html.ActionLink("Nuevo Idioma", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idIdioma)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idioma)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idIdioma)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idioma)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.idIdioma }) |
            @Html.ActionLink("Detalles", "Details", new { id = item.idIdioma }) |
            @Html.ActionLink("Eliminar", "Delete", new { id = item.idIdioma }, new { onclick = "return confirm('Desea eliminar este idioma?');" })
        </td>
    </tr>
    }

</table>
