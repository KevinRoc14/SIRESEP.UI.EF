@model IEnumerable<SIRESEP.DATOS.tbInfoLaboral>

@{
    ViewBag.Title = "Index";
}

<h2>Informacion Laboral</h2>


<p>
    @Html.ActionLink("Nueva Informacion Laboral", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idInfoLaboral)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.profesion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tiempoExperiencia)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idInfoLaboral)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.profesion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tiempoExperiencia)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.idInfoLaboral }) |
            @Html.ActionLink("Detalles", "Details", new { id = item.idInfoLaboral }) |
            @Html.ActionLink("Eliminar", "Delete", new { id = item.idInfoLaboral }, new { onclick = "return confirm('Desea eliminar este informacion Laboral?');" })
        </td>
    </tr>
    }

</table>
