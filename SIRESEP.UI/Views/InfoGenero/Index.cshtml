@model IEnumerable<SIRESEP.DATOS.tbInfoGenero>

@{
    ViewBag.Title = "Index";
}

<h2>Generos</h2>

<p>
    @Html.ActionLink("Nuevo Genero", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idGenero)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.genero)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idGenero)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.genero)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.idGenero }) |
            @Html.ActionLink("Detalles", "Details", new { id = item.idGenero }) |
            @Html.ActionLink("Eliminar", "Delete", new { id = item.idGenero }, new { onclick = "return confirm('Desea eliminar este genero?');" })
        </td>
    </tr>
}

</table>
